{
    "name": "toolstud-io/colorjizz-php8",
    "description": "Classes for manipulating colors, converting to different formats and finding color harmonies",
    "homepage": "https://github.com/toolstud-io/ColorJizz-PHP8",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "type": "library",
    "autoload": {
        "psr-0": {
            "MischiefCollective": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.14"
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
        ]
    },
    "version": "2.0.4"
}
